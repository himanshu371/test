<apex:page controller="VFDemo5Controller" >
    <apex:form >
     <table border="1">
            <tr>
             <td>Account Name</td>
                <td><apex:inputField value="{!acc.Name}"/></td>
            </tr>
            <tr>
             <td>Rating</td>
                <td><apex:inputField value="{!acc.Rating}"/></td>
            </tr>
            <tr>
             <td><apex:commandButton value="Save" action="{!save}"/></td>
                <td><apex:commandButton value="Cancel" action="{!cancel}"/></td>
            </tr>
        </table>
    </apex:form>
</apex:page>